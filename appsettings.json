{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "UserInput": {
    "KeyMatrix": {
      "Layout": [
        [ "1", "2", "3", "A" ],
        [ "4", "5", "6", "B" ],
        [ "7", "8", "9", "C" ],
        [ "*", "0", "#", "D" ]
      ],
      "GPIO": {
        "OutputPins": [5, 6, 13, 19],
        "InputPins": [12, 16, 20, 21],
        "ScanIntervalMs": 20,
        "PinMode": "InputPullDown" 
      }
    }
  }
}
